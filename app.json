{
  "expo": {
    "name": "TrashVax",
    "slug": "TrashVax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/trashvax_icon.png",
    "splash": {
      "image": "./assets/trashvax_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/trashvax_adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "icon": "./assets/trashvax_icon.png",
      "package": "io.trashvax.client",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/trashvax_favicon.png"
    },
    "notification": {
      "icon": "./assets/trashvax_icon.png"
    }
  }
}
